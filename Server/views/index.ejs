<% layout('layout') -%>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            DashBoard
            <small>Preview</small>
          </h1>
          <ol class="breadcrumb">
            <li><i class="fa fa-dashboard"></i> Home</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
	  <div class="row">
          <!-- Small boxes (Stat box) -->
	    <div class="col-md-6">
              <div class="box">
		<div class="box-header">
                  <p><h3 class="box-title">비콘 현황</h3></p>
                </div><!-- /.box-header -->
                <div class="box-body">
                  <table class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>No.</th>
                        <th>Mac Address</th>
                        <th>Location</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% data.forEach( function(item, index) { %>
                      <tr>
                        <td><%= index + 1 %></td>
                        <td><%= item.MAC %></td>
                        <td><%= item.LOC_CODE %></td>
                      </tr>
                      <% }); %>
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
            </div>
            <div class="col-md-6">
              <div class="box">
                <div class="box-header">
                  <h3 class="box-title">Logs</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                  <table class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th style=text-align:center>UserID</th>
                        <th>UserName</th>
                        <th>Location</th>
                        <th>Time</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% data1.forEach( function(item, index) { %>
                      <tr>
                        <td style=text-align:center><%= item.USERID %></td>
                        <td><%= item.USERNAME %></td>
                        <td><%= item.LOCATIONNAME %></td>
                        <td><%= item.DATE %></td>
                      </tr>
                      <% }); %>
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
            </div><!-- /.col -->
          </div><!-- /.row -->
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
