<% layout('layout') -%>
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Log Data
            <small>logs tables</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/"><i class="fa fa-book"></i>Home</a></li>
            <li class="active">Logs</li>
          </ol>
        </section>

        <!-- Main content -->
      <!-- jQuery 2.1.4 -->
      <script src="/js/plugins/jQuery/jQuery-2.1.4.min.js"></script>
      <!-- DataTables -->
      <script src="/js/plugins/datatables/jquery.dataTables.min.js"></script>
      <script src="/js/plugins/datatables/dataTables.bootstrap.min.js"></script>
      <!-- page script -->
      <script>
      $(function () {
        $("#example1").DataTable();
        $('#example2').DataTable({
          "paging": true,
          "lengthChange": false,
          "searching": true,
          "ordering": false,
          "info": true,
          "autoWidth": false
        });
      });
      </script>
        <section class="content">
          <div class="row">
            <div class="col-xs-12">
              <div class="box">
                <div class="box-header">
                  <h3 class="box-title">Logs</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                  <table id="example2" class="table table-bordered table-hover">
                    <thead>
                      <tr>
			<th>#</th>
                        <th>UserID</th>
                        <th>UserName</th>
                        <th>Location</th>
                        <th>TIME</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% data.forEach(function (item, index) { %>
                      <tr>
                        <td><%= data.length - index %></td>
                        <td><%= item.USERID %></td>
                        <td><%= item.USERNAME %></td>
                        <td><%= item.LOCATIONNAME %></td>
                        <td><%= item.DATE %></td>
                      </tr>
                      <% }); %>
                    </tbody>
                    <tfoot>
                      <tr>
                        <th>#</th>
                        <th>UserID</th>
                        <th>UserName</th>
                        <th>Location</th>
                        <th>TIME</th>
                      </tr>
                    </tfoot>
                  </table>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
            </div><!-- /.col -->
          </div><!-- /.row -->
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
